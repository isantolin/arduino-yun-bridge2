config general 'general'
    option enabled '1'
    option debug '0'
    option serial_port '/dev/ttyATH0'
    option serial_baud '115200'
    option mqtt_host '127.0.0.1'
    option mqtt_port '8883'
    option mqtt_tls '1'
    option mqtt_cafile '/etc/ssl/certs/ca-certificates.crt'
    option mqtt_topic 'br'
    option file_system_root '/root/yun_files'
    option process_timeout '10'
    option serial_shared_secret 'changeme123'  # CHANGE THIS TO A UNIQUE VALUE
