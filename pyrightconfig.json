{
    "typeCheckingMode": "standard",
    "strict": [
        "openwrt-yun-bridge/yunbridge",
        "openwrt-yun-examples-python/yunbridge_client",
        "openwrt-yun-core/scripts"
    ],
    "include": [
        ".venv",
        ".tox",
        "openwrt-yun-bridge/yunbridge",
        "openwrt-yun-examples-python/yunbridge_client",
        "openwrt-yun-core/scripts"
    ],
    "stubPath": "stubs",
    "extraPaths": [
        "openwrt-yun-bridge"
    ],
    "exclude": [
        "openwrt-sdk",
        ".tox",
        ".venv",
        "**/__pycache__",
        "**/node_modules",
        "**/.*",
        ".venv_debug"
    ]
}
